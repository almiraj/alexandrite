{
  "name": "alexandrite",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"node ./bin/www\" \"npm run tsc:server\" \"npm run tsc:client\"",
    "postinstall": "typings install && cd public/js && npm install",
    "tsc:server": "tsc -w",
    "tsc:client": "tsc -w -p ./public/js/",
    "typings": "typings"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "typescript": "^1.8.10",
    "typings":"^1.0.4"
  }
}
